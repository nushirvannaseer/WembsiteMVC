@using Wembsite.Models;
@model IEnumerable<Wembsite.Models.User>

@{
    ViewBag.Title = "Following";
}
<h2>Following</h2>
<link href="../../Content/LoginStyles.css" rel="stylesheet" type="text/css" />

<table class="table">
    <tr>
        <th>
            Users
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.username, "../AllUsers/NonSessionUserProfile", new { username = item.username })
            </td>

        </tr>
    }

</table>
@Html.ActionLink("Back to Profile Page", "Profile")


